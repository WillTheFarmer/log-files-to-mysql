      "process_list": ["nginxErrorLoad","nginxErrorParse","nginxErrorImport"]
    },
    {
      "id": "nginxCombined",
      "name": "NGINX Access Format",
      "description": "Apache Combined format",
      "status": "Active",
      "print": 2,
      "path": "data/nginx_combined/",
      "recursive": false,
