  "observers": [
    {
      "id": "apacheError",
      "name": "Apache Error Standard",
      "description": "Apache Error format",
      "status": "Active",
      "print": 2,
      "path": "data/apache_error/",
      "recursive": false,
      "interval": 15,
      "process_list": ["apacheErrorLoad","apacheErrorParse","apacheErrorImport"]
    },
    {
      "id": "apacheCommon",
      "name": "Apache Common",
      "description": "Apache Common format - LOAD ONLY",
      "status": "Active",
      "print": 2,
      "path": "data/apache_common/",
      "recursive": false,
      "interval": 25,
      "process_list": ["apacheCombinedLoad"]
    },
    {
      "id": "apacheCombined",
      "name": "Apache Combined",
      "description": "Apache Combined format - No Import process",
      "status": "Active",
      "print": 2,
      "path": "data/apache_combined/",
      "recursive": false,
      "interval": 25,
      "process_list": ["apacheCombinedLoad","apacheCombinedParse"]
    },
    {
      "id": "apacheVhost",
      "name": "Apache Vhost",
      "description": "Apache Vhost format - LOAD ONLY",
      "status": "Active",
