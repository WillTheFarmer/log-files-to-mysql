    {
      "id": "apacheErrorLoad",
      "name": "Apache Error Load",
      "description": "Error Standard log files loading.",
      "moduleName": "fileLoader",
      "group": "Apache Error",
      "status": "Active",
      "attributes": {
        "print": 2,
        "log_format": "apacheError",
        "load_table": "load_error_apache",
