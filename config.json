{
  "mysql": {
    "host": "localhost",
    "port": 3606,
    "user": "http_upload",
    "password": "Just4TheData",
    "schema": "http_logs"
  },
  "backup": {
    "days": 0,
    "path": "data/backup_files/"
  },
  "observers": [
    {
      "id": "apache_error",
      "name": "Apache error Default format",
      "description": "watch for Apache error files in Default format",
      "status": "Active",
      "print": 2,
      "path": "data/apache_error/",
      "recursive": false,
      "interval": 15,
      "process_list": ["apacheErrorLoad","apacheErrorParse","apacheErrorImport"]
    },
    {
      "id": "apache_combined",
      "name": "Apache access Combined format",
      "description": "watch for Apache access files in Combined format - Notice no Import process",
      "status": "Active",
      "print": 2,
      "path": "data/apache_combined/",
      "recursive": false,
      "interval": 25,
      "process_list": ["apacheCombinedLoad","apacheCombinedParse"]
    },
    {
      "id": "apache_vhost",
      "name": "Apache access Vhost format",
      "description": "watch for Apache access files in Vhost format - LOAD ONLY",
      "status": "Active",
      "print": 2,
      "path": "data/apache_vhost/",
      "recursive": false,
      "interval": 15,
      "process_list": ["apacheVhostLoad"]
    },
    {
      "id": "apache_csv2mysql",
      "name": "Apache access CSV2MySQL format",
      "description": "watch for Apache access files in CSV2MySQL format",
      "status": "Active",
      "print": 2,
      "path": "data/apache_csv2mysql/",
      "recursive": false,
      "interval": 15,
      "process_list": ["apacheCsv2mysqlLoad","apacheCsv2mysqlParse","apacheCsv2mysqlImport"]
    },
    {
      "id": "nginx_error",
      "name": "NGINX error Default format",
      "description": "watch for NGINX error files in Default format",
      "status": "Active",
      "print": 2,
      "path": "data/nginx_error/",
      "recursive": false,
      "interval": 15,
      "process_list": ["nginxErrorLoad","nginxErrorParse","nginxErrorImport"]
    },
    {
      "id": "nginx_default",
      "name": "NGINX access Default format",
      "description": "watch for NGINX files in Default format",
      "status": "Active",
      "print": 2,
      "path": "data/nginx_default/",
      "recursive": false,
      "interval": 15,
      "process_list": ["nginxDefaultLoad","nginxDefaultParse","nginxDefaultImport"]
    }
  ],
  "processes": [
    {
      "id": "apacheErrorLoad",
      "name": "Apache error in Default file load",
      "description": "Sucks up all error files in repository data folder",
      "module_name": "data_file_loader",
      "group": "Apache Error",
      "status": "Active",
      "attributes": {
        "print": 2,
        "log_format": "apache_error",
        "load_table": "load_error_apache_default",
        "path": "data/apache_error/**/*error*.*",
        "recursive": true,
        "server": "mydomain.com",
        "server_port": 443
      }
    },
    {
      "id": "apacheErrorParse",
      "name": "Apache Error Parse",
      "description": "Apache Error parse process at database",
      "module_name": "database_module",
      "group": "Apache Error",
      "status": "Active",
      "attributes": {
        "print": 2,
        "module_name": "parse_error_apache_default",
        "module_parm1": "python"
      }
    },
    {
      "id": "apacheErrorImport",
      "name": "Apache Error Import",
